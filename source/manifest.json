{
  "name": "Get audio url",
  "description": "GetAudioUrl",
  "version": "2.0",
  "manifest_version": 3,
  "permissions": [
    "contextMenus",
    "declarativeContent"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.ldoceonline.com/*"],
      "js": ["content-script.js"]
    }
  ]
}