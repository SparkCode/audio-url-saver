{
    "content_scripts": [
        {
          "matches": [
            "https://*.ldoceonline.com/*",
            "https://dictionary.cambridge.org/*"
          ],
          "js": ["content-script.js"]
        }
      ],
   "description": "Get audio URLs from Longman and Cambridge dictionaries",
   "manifest_version": 2,
   "name": "Get audio url",
   "permissions": [ "tabs", "contextMenus"],
   "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
   "version": "1.0"
}
